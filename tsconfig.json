{
  "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "moduleResolution": "node",
      "sourceMap": true,
      "baseUrl": ".",
      "paths": {
          "*": [
              "node_modules/*"
          ]
      },
      "lib": [
          "esnext"
      ]
  },
  "include": [
      "**/*.ts",
      "bin/www"
  ],
  "exclude": [
      "node_modules/**/*"
  ]
}